{
  "name": "cuby-generator",
  "description": "A tool to generate Cuby textures, avatars and wallpapers.",
  "version": "1.1.0",
  "private": true,
  "contributors": [
    {
      "name": "Thorn-Welf Walli",
      "email": "lammpee@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ThornWalli/cuby-generator"
  },
  "scripts": {
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:es": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint": "npm run lint:es && npm run lint:style",
    "test": "npm run lint && npm run generate"
  },
  "dependencies": {
    "@js-basics/vector": "2.1.0",
    "compressorjs": "^1.1.1",
    "file-saver": "2.0.5",
    "husky": "8.0.3",
    "nuxt": "2.18.1",
    "nuxt-speedkit": "2.2.9",
    "pinst": "3.0.0",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.7.2",
    "seedrandom": "3.0.5",
    "vue-svg-loader": "0.16.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.8",
    "@commitlint/config-conventional": "^17.8.1",
    "@nuxt/postcss8": "^1.1.3",
    "@nuxtjs/eslint-config": "10.0.0",
    "@nuxtjs/eslint-module": "3.1.0",
    "@nuxtjs/pwa": "3.3.5",
    "@nuxtjs/stylelint-module": "4.2.2",
    "commitlint": "^17.8.1",
    "core-js": "^3.38.1",
    "eslint": "8.57.1",
    "eslint-plugin-no-secrets": "^0.9.0",
    "eslint-plugin-no-unsanitized": "^4.0.1",
    "eslint-plugin-nuxt": "3.2.0",
    "eslint-plugin-security": "^1.5.0",
    "lint-staged": "^13.3.0",
    "postcss-html": "^1.5.0",
    "stylelint": "14.16.1",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "26.0.0"
  }
}
